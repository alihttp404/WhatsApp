<Window x:Class="WhatsApp.Views.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WhatsApp.Views"
        mc:Ignorable="d"
        Title="Register" Height="184" Width="355">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Command="Login" Content="Log In" Style="{StaticResource MaterialDesignRaisedButton}" Height="40" Width="150" Margin="0,0,0,10"/>
            <Button Content="Register" Style="{StaticResource MaterialDesignRaisedButton}" Height="40" Width="150"/>
        </StackPanel>
        <Frame x:Name="MainFrame" Source="Login.xaml"/>
    </Grid>
</Window>
